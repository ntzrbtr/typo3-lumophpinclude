<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.sheet_general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <transfer_get>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.transfer_get</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </transfer_get>
                    <transfer_post>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.transfer_post</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </transfer_post>
                    <transfer_cookies>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.transfer_cookies</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </transfer_cookies>
                    <script_type>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.script_type</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.script_type_file</numIndex>
                                        <numIndex index="1">file</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.script_type_url</numIndex>
                                        <numIndex index="1">url</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </script_type>
                    <script_file>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.script_file</label>
                            <config>
                                <type>group</type>
                                <internal_type>file</internal_type>
                                <allowed>php,inc</allowed>
                                <max_size>500</max_size>
                                <uploadfolder>uploads/</uploadfolder>
                                <maxitems>1</maxitems>
                                <size>1</size>
                                <selectedListStyle>width:180px</selectedListStyle>
                            </config>
                        </TCEforms>
                    </script_file>
                    <script_url>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.script_url</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </script_url>
                    <strip_non_body>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.strip_non_body</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </strip_non_body>
                    <strip_non_marked>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.strip_non_marked</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </strip_non_marked>
                    <marker_start>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.marker_start</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                            </config>
                        </TCEforms>
                    </marker_start>
                    <marker_end>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.marker_end</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                            </config>
                        </TCEforms>
                    </marker_end>
                    <wrap_in_div>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.wrap_in_div</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </wrap_in_div>
                    <replace_pid>
                        <TCEforms>
                            <label>LLL:EXT:lumophpinclude/pi1/locallang.xml:lumophpinclude.pi1.flexform.replace_pid</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </replace_pid>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>